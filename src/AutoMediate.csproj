<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.8" />
  </ItemGroup>
	<PropertyGroup>
		<Version> 0.1.1</Version>
		<Authors>Mohamed Hassan Ashmawy</Authors>
		<PackageVersion>0.1.1</PackageVersion>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageTags> AutoMediate </PackageTags>
		<RepositoryUrl> https://github.com/mashmawy/AutoMediate.git </RepositoryUrl>
		<PackageDescription> AutoMediate – A lightweight, dependency-injection friendly mediator library for decoupling request handling in .NET applications. </PackageDescription>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\LICENSE.txt" Pack="true" PackagePath=""/>
		<None Include="..\README.md" Pack="true" PackagePath=""/>
	</ItemGroup>
</Project>
